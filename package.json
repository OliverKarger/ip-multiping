{
  "name": "@oliverkarger/ip-multiping",
  "version": "1.0.0",
  "description": "Multi-IP Testing and Ping Tool",
  "main": "src/ip-multiping.ts",
  "bin": "src/ip-multiping.ts",
  "scripts": {
    "start": "babel-node src/ip-multiping.ts --extensions \".ts\"",
    "pkg": "pkg --config package.json ./pureJS/ip-multiping.js",
    "typescript": "tsc",
    "build": "npm run typescript && npm run pkg",
    "eslint": "eslint src/**/*",
    "prettier": "prettier --write ."
  },
  "keywords": [],
  "author": {
    "name": "Oliver Karger",
    "email": "mail@oliver-karger.de"
  },
  "license": "SEE LICENSE IN ./License",
  "dependencies": {
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "enquirer": "^2.3.6",
    "figures": "^3.2.0",
    "linebyline": "^1.3.0",
    "log-symbols": "^5.0.0",
    "ping": "^0.4.1",
    "signale": "^1.4.0",
    "validate-ip-node": "^1.0.6",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^16.7.12",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "pkg": "^5.2.1",
    "prettier": "^2.2.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "pkg": {
    "assets": [
      "pureJS/**/*"
    ],
    "targets": [
      "node12-macos-x64",
      "node12-win-x64"
    ],
    "output": "./build"
  }
}
